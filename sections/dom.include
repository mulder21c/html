    <li><{button}></li>
    <li><{canvas}></li>
    <li><{cite}></li>
    <li><{code}></li>
    <li><{data}></li>
    <li><{datalist}></li>
    <li><{del}></li>
    <li><{dfn}></li>
    <li><{em}></li>
    <li><{embed}></li>
    <li><{i}></li>
